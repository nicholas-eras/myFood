#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nicholas/myFood/backend/node_modules/.pnpm/typeorm@0.3.24_pg@8.16.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1.11.31_@types+node@22.15.30_typescript@5.8.3_/node_modules/typeorm/node_modules:/home/nicholas/myFood/backend/node_modules/.pnpm/typeorm@0.3.24_pg@8.16.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1.11.31_@types+node@22.15.30_typescript@5.8.3_/node_modules:/home/nicholas/myFood/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nicholas/myFood/backend/node_modules/.pnpm/typeorm@0.3.24_pg@8.16.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1.11.31_@types+node@22.15.30_typescript@5.8.3_/node_modules/typeorm/node_modules:/home/nicholas/myFood/backend/node_modules/.pnpm/typeorm@0.3.24_pg@8.16.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1.11.31_@types+node@22.15.30_typescript@5.8.3_/node_modules:/home/nicholas/myFood/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.24_pg@8.16.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1.11.31_@types+node@22.15.30_typescript@5.8.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.24_pg@8.16.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1.11.31_@types+node@22.15.30_typescript@5.8.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
